        {
          "type": "HorizontalLayout",
          "elements": [
            {
              "type": "Control",
              "scope": "#/properties/edgeElement"
            },
            {
              "type": "Control",
              "scope": "#/properties/edgeTransition"
            }
          ]
        }
      ]
    },
    {
      "type": "Group",
      "label": "Element to Align",
      "elements": [
        {
          "type": "HorizontalLayout",
          "elements": [
            {
              "type": "Control",
              "scope": "#/properties/elementToAlign"
            },
            {
              "type": "Control",
              "scope": "#/properties/transitionToAlign"
            }
          ]
        }
      ]
    },
    {
      "type": "Control",
      "scope": "#/properties/outputFolder"
    }
