{
  "type": "VerticalLayout",
  "elements": [
    {
      "type": "Group",
      "elements": [
        {
          "type": "VerticalLayout",
          "elements": [
            {
              "type": "Control",
              "scope": "#/properties/scanRange",
              "label": "Scan Range",
              "options": {
                "useScanRangeControl": true
              }
            },
            {
              "type": "Control",
              "scope": "#/properties/method"
            },
            {
              "type": "Control",
              "scope": "#/properties/normalise"
            }
          ]
        }
      ]
    },
    {
      "type": "Group",
      "label": "Edge Element",
